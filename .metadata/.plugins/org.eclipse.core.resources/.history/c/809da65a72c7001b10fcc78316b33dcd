package com.cg.pack.repository;



import java.util.List;


import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.cg.pack.entities.Pack;

@Repository
public interface PackRepository extends JpaRepository<Pack,Long>{


	List<Pack> findByGreaterThanAmount(double amount);

	List<Pack> findPacksInAscendingOrderByCost(double amount);

	List<Pack> findPacksInAscendingOrderByDaysValidity();

	List<Pack> findRechargesCountInDescendingOrder();
	
	
	
	
	

	
}
